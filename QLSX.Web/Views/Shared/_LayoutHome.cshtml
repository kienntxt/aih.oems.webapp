    <!-- END: Page Vendor JS-->
    <!-- BEGIN: Theme JS-->

    <script src="~/Content/app-assets/js/core/app-menu.js"></script>

    <script src="~/Content/app-assets/js/core/app.js"></script>
    <!-- END: Theme JS-->
    <!-- BEGIN: Page JS-->

    <script src="~/Content/app-assets/js/scripts/pages/dashboard-analytics.js"></script>
