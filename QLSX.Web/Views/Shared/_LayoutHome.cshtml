        @Html.Action("Index", "Navbar")
    </nav>
    <!-- END: Header-->
    <!-- BEGIN: Main Menu-->
    <div class="main-menu menu-fixed menu-dark menu-accordion menu-shadow" data-scroll-to-active="true">
        @Html.Action("Index", "Menu")
    </div>
    <!-- END: Main Menu-->
    <!-- BEGIN: Content-->
    @RenderBody()
    <!-- END: Content-->

