    <!-- BEGIN: Theme CSS-->

    <link href="~/Content/app-assets/css/bootstrap.css" rel="stylesheet" type="text/css" />

    <link href="~/Content/app-assets/css/bootstrap-extended.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/app-assets/css/colors.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/app-assets/css/components.css" rel="stylesheet" type="text/css" />
